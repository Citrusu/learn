{
    "compilerOptions": {
        "sourceMap": false,
        "strictNullChecks": false,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "target": "es6",
        "lib": ["es6", "dom", "es7"],
        "noImplicitAny": false,
        "moduleResolution": "node",
        "jsx": "react",
        "allowJs": true,
        "skipLibCheck": true,
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "path":{
        
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
